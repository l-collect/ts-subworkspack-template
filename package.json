{
  "name": "@oitq/ts-subworkspace-template",
  "version": "1.0.0",
  "description": "ts workspace template",
  "main": "lib/index.js",
  "scripts": {
    "compile": "tsc",
    "start": "node .",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "alias": "tsc-alias -p tsconfig.json",
    "build": "npm run compile && npm run alias",
    "pub": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "lib"
  ],
